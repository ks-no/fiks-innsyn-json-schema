{
  "id": "https://api.fiks.ks.no/innsyn/schema/domain/sak/aktivitet.v1.json",
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "Aktivitet V1",
  "description": "Aktivitet utført på en melding",
  "type": [
    "object",
    "null"
  ],
  "additionalProperties": false,
  "required": [
    "hendelse",
    "dato",
    "part"
  ],
  "properties": {
    "hendelse": {
      "description": "Kort navn på aktivitet",
      "$ref": "../../types/string.not.blank.json"
    },
    "dato": {
      "description": "Dato for når aktiviteten ble utført på ISO 8601-format",
      "type": "string",
      "format": "date-time",
      "examples": [
        "2022-03-16T14:10:11+00:00"
      ]
    },
    "part": {
      "description": "Part som utførte aktiviteten",
      "$ref": "part.v1.json"
    }
  }
}