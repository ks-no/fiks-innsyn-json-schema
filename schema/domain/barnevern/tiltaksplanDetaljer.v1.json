{
  "$id": "https://api.fiks.ks.no/innsyn/schema/domain/barnevern/tiltaksplanDetaljer.v1.json",
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "Barnevern TiltaksplanDetaljer V1",
  "description": "Detaljer for en tiltaksplan",
  "required": [
    "tiltaksstatus",
    "startDato",
    "sluttDato",
    "delplaner"
  ],
  "properties": {
    "tiltaksstatus": {
      "enum": [
        "AKTIV",
        "AVSLUTTET",
        "MANUELT AVSLUTTET"
      ]
    },
    "startDato": {
      "type": "string",
      "format": "date-time"
    },
    "sluttDato": {
      "type": "string",
      "format": "date-time"
    },
    "delplaner": {
      "type": "array",
      "items": {
        "$ref": "delplan.v1.json"
      }
    }
  }
}