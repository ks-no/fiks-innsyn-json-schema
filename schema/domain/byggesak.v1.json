{
  "id": "https://api.fiks.ks.no/innsyn/schema/domain/byggesak.v1.json",
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "Innsyn Byggesak V1",
  "description": "Melding som beskriver en byggesak",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "tittel",
    "saksdato"
  ],
  "definitions": {
    "ByggesakSaksnummerV1": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "saksaar",
        "sakssekvensnummer"
      ],
      "properties": {
        "saksaar": {
          "type": "string"
        },
        "sakssekvensnummer": {
          "type": "string"
        }
      }
    },
    "ByggesakTiltakstypeV1": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "kodeverdi",
        "kodebeskrivelse"
      ],
      "properties": {
        "kodeverdi": {
          "$ref": "../types/string.not.blank.json"
        },
        "kodebeskrivelse": {
          "$ref": "../types/string.not.blank.json"
        }
      }
    },
    "ByggesakMatrikkelnummerV1": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "kommunenummer",
        "gardsnummer",
        "bruksnummer",
        "festenummer",
        "seksjonsnummer"
      ],
      "properties": {
        "kommunenummer": {
          "type": "string",
          "pattern": "^\\d{4}$"
        },
        "gardsnummer": {
          "type": "string",
          "pattern": "^[0-9]{1,6}$"
        },
        "bruksnummer": {
          "type": "string",
          "pattern": "^[0-9]{1,6}$"
        },
        "festenummer": {
          "type": "string",
          "pattern": "^[0-9]{1,6}$"
        },
        "seksjonsnummer": {
          "type": "string",
          "pattern": "^[0-9]{1,6}$"
        }
      }
    },
    "ByggesakDokumentV1": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "tittel",
        "dokumenttype",
        "nedlastingslenke",
        "dokumentsdato",
        "vedtaksstatus",
        "vedtaksdato",
        "versjon",
        "systemId",
        "erstatterTidligereSystemId",
        "mimeType"
      ],
      "properties": {
        "tittel": {
          "description": "Beskrivende tittel på dokumentet",
          "$ref": "../types/string.not.blank.json"
        },
        "dokumenttype": {
          "$ref": "#/definitions/ByggesakDokumentTypeV1"
        },
        "nedlastingslenke": {
          "description": "Referanse til dokument med uri",
          "type": "string"
        },
        "dokumentsdato": {
          "$ref": "../types/int64.json",
          "description": "Dato som er påført selve dokumentet. Uttrykket som millisekunder etter epoch (1970-01-01 00:00:00, UTC). Kan være negativ."
        },
        "vedtaksstatus": {
          "description": "Referanse til dokument med uri",
          "type": "string"
        },
        "vedtaksdato": {
          "$ref": "../types/int64.json",
          "description": "Dato for vedtak. Uttrykket som millisekunder etter epoch (1970-01-01 00:00:00, UTC). Kan være negativ."
        },
        "versjon": {
          "type": "string"
        },
        "systemId": {
          "type": "string"
        },
        "erstatterTidligereSystemId": {
          "type": "string"
        },
        "size": {
          "$ref": "../types/int.nullable.json",
          "description": "Størrelse på fil i bytes"
        },
        "mimeType": {
          "type": "string"
        }
      }
    },
    "ByggesakDokumentTypeV1": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "kodeverdi",
        "kodebeskrivelse"
      ],
      "properties": {
        "kodeverdi": {
          "$ref": "../types/string.not.blank.json"
        },
        "kodebeskrivelse": {
          "$ref": "../types/string.not.blank.json"
        }
      }
    }
  },
  "properties": {
    "tittel": {
      "$ref": "../types/string.not.blank.json",
      "description": "Tittel for byggesaken"
    },
    "beskrivelse": {
      "$ref": "../types/string.nullable.json",
      "description": "Beskrivelse av byggesaken"
    },
    "mappeId": {
      "$ref": "../types/string.nullable.json",
      "description": ""
    },
    "sakstatus": {
      "$ref": "../types/string.nullable.json",
      "description": ""
    },
    "saksdato": {
      "$ref": "../types/int64.json",
      "description": "Dato saken ble opprettet. Uttrykket som millisekunder etter epoch (1970-01-01 00:00:00, UTC). Kan være negativ."
    },
    "saksansvarlig": {
      "$ref": "../types/string.nullable.json",
      "description": "Saksbehandler som er ansvarlig for saken"
    },
    "administrativEnhet": {
      "$ref": "../types/string.nullable.json",
      "description": "Avdeling, team eller etat som behandler saken"
    },
    "adresse": {
      "$ref": "../types/string.nullable.json",
      "description": "Adresse til tiltaket",
      "examples": [
        "Øvre Eksempelveien 35, 5073 BERGEN"
      ]
    },
    "avsluttetDato": {
      "$ref": "../types/int64.nullable.json",
      "description": "Dato når saken er avsluttet"
    },
    "saksnummer": {
      "$ref": "#/definitions/ByggesakSaksnummerV1"
    },
    "tiltakstype": {
      "$ref": "#/definitions/ByggesakTiltakstypeV1"
    },
    "tiltakshaver": {
      "$ref": "../types/string.nullable.json",
      "description": "Navn på tiltakshaver. Tiltakshaver er eier av søknaden og har et overordnet ansvar for det som bygges"
    },
    "ansvarligSoker": {
      "$ref": "../types/string.nullable.json",
      "description": "Navn på ansvarlig søker. Ansvarlig søker har påtatt seg ansvaret for byggesøknaden og all kommunikasjon med kommunens byggesaksavdeling"
    },
    "matrikkelnummer": {
      "$ref": "#/definitions/ByggesakMatrikkelnummerV1"
    },
    "dokumentlistebeskrivelse": {
      "$ref": "../types/string.nullable.json",
      "description": "En beskrivelse av hva dokumentlisten inneholder. Feks om det er utvalge dokumenter i en sak eller alle dokumenter"
    },
    "dokumenter": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/ByggesakDokumentV1"
      }
    },
    "korrelasjonId": {
      "$ref": "korrelasjon.id.json"
    },
    "forelder": {
      "$ref": "forelder.json"
    }
  }
}